import math

num_dec=float(input("ingrese un numero decimal"))
num_abaj=math.ceil(num_dec)
num_arrib=math.floor(num_dec)
print(f"el numero redondeado para arriba es {num_arrib} y el numero para abajo es {num_abaj}")
